    "**/.DS_Store": true,
    "**/.cache": true
  },
  "search.exclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/dist/**": true,
    "**/__pycache__": true,
    "**/__init__.py": true,
    "**/*.pyc": true,
    "**/.DS_Store": true,
    "**/.cache": true
  },
  "typescript.tsserver.maxTsServerMemory": 3072,
